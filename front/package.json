{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "tailwindcss": "^3.0.8",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@emotion/eslint-plugin": "^11.7.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-plugin-macros": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "@emotion"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 80,
    "semi": false
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,md,css,html}": "prettier --write"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
